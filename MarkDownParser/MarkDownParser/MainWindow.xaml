<Window x:Class="MarkDownParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="119*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*" />
            <ColumnDefinition Width="50*" />
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="0" TextChanged="TextBox_TextChanged_1" Name="txt1" Grid.Row="1" AcceptsReturn="True" AcceptsTab="True"/>
        <WebBrowser Name="webBrowser1" Grid.Column="1" Grid.Row="1" />
        <Menu Name="menu" Grid.ColumnSpan="2" Grid.Row="0">
            <ItemsControl>
                <MenuItem>toto</MenuItem>
                <MenuItem>titi</MenuItem>
                <MenuItem>tete</MenuItem>
            </ItemsControl>
        </Menu>
    </Grid>
</Window>
